{
  "name": "markdown-editor",
  "version": "1.0.0",
  "description": "A real-time markdown editor made with Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node .",
    "dev": "concurrently \"nodemon -e js,mjs,cjs,ejs,json\" \"npm run scss\"",
    "scss": "sass --watch src/scss/style.scss:public/css/style.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GustavoSouza123/node-markdown-editor.git"
  },
  "keywords": [
    "markdown",
    "editor",
    "node",
    "javascript"
  ],
  "author": "Gustavo Souza",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "redis": "^4.6.14",
    "sass": "^1.77.5",
    "share": "^0.7.40",
    "showdown": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3"
  }
}
